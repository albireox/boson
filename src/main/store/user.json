{
  "connection": {
    "name": "APO",
    "observatory": "APO",
    "program": "apo",
    "user": "",
    "host": "sdss5-hub.apo.nmsu.edu",
    "port": 9877,
    "needsAuthentication": true,
    "httpHost": "sdss5-hub.apo.nmsu.edu",
    "httpPort": 80
  },
  "windows": {
    "openWindows": ["main"]
  },
  "interface": {
    "mode": "system",
    "saveWindows": true,
    "saveOnlyOnRequest": false
  },
  "guider": {
    "xpaset": "/usr/local/bin/xpaset",
    "fullImage": false,
    "colormap": "heat",
    "scale": "linear",
    "scalelim": "zscale",
    "titlebarStatus": true,
    "refreshInterval": 10
  },
  "hal": {
    "allowGotoFieldAutoMode": true,
    "syncStages": false
  },
  "log": {
    "wrap": false,
    "saveState": true,
    "showInternal": false,
    "highlightCommands": "mine"
  },
  "chat": {
    "notifications": true
  },
  "audio": {
    "mode": "on",
    "muted": false,
    "minimal": ["error"],
    "sounds": {
      "error": "error.wav",
      "error_serious": "synth_error_long.wav",
      "warning": "click.wav",
      "axis_halt": "synth_strings_short.wav",
      "axis_slew": "marimba.wav",
      "exposure_start": "woodblock.wav",
      "exposure_end": "bell_soft_long.wav"
    },
    "user_sounds": ["bar_long.wav", "bell_soft_long.wav", "bell_soft_short.wav", 
                      "buzz_error_long.wav", "buzz_error_short.wav", "chime_long.wav", 
                      "click.wav", "cowbell_high.wav", "cowbell_low.wav", "error.wav", 
                      "high_buzz_error.wav", "hit_long.wav", "jingle_bell.wav", "low_error.wav",
                      "marimba.wav", "marimba_sharp.wav", "synth_error_long.wav",  
                      "synth_soft_short.wav","synth_string_echo.wav", "synth_strings_long.wav",
                      "synth_strings_short.wav","woodblock.wav"]
  },
  "maxLogMessages": 50000,
  "updateChannel": "stable",
  "profiles": {}
}
